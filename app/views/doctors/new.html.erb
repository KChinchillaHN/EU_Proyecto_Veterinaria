<h1>Crear nuevo Doctor</h1>
<p><em>Ingrese los siguientes datos: </em></p>

<%= render partial: "shared/errors", locals: { obj: @doctor } %>

<%= form_for @doctor do |f| %>
	<%= render partial: "form", locals: {f: f} %>

<div class="btn-group" role="group" aria-label="Acciones">
	<%= button_tag(type: 'submit', class: "btn btn-default") do %>
			<i class="icon-ok icon-white"></i> Crear Doctor
<% end %>
		<%= link_to "Cancelar", login_path, class: "btn btn-default"%>

<%- end %>