<% if appointments.any? %>
	<div>
		<h2>Citas hechas: </h2>
		<table border="2">
			<thead>
				<th>ID</th>
				<th>Fecha</th>
				<th>Monto Pagado</th>
			</thead>
			<tbody>
				<%= render appointments %>
			</tbody>
		</table>
	</div>
<% else %>
	<p><em>No tiene citas registradas aun.</em></p>
<% end %>